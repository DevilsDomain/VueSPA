{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "plugins": ["vue", "@typescript-eslint", "prettier"],
  "rules": { "prettier/prettier": "error" },
  "extends": [
    "plugin:vue/vue3-essential",
    "standard-with-typescript",
    "prettier",
    "@vue/typescript/recommended"
  ]
}
